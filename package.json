{
  "name": "scm-ui-vue",
  "version": "0.0.1",
  "main": "dist/scm-ui-vue.umd.js",
  "module": "dist/scm-ui-vue.es.js",
  "unpkg": "dist/scm-ui-vue.life.js",
  "browser": "dist/scm-ui-vue.cjs.js",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/scm-ui.es.js",
      "require": "./dist/scm-ui.cjs.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vue-tsc && vite build",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview",
    "lint": "npm run lint:eslint & npm run lint:stylelint",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss,sass,less,vue}\" --fix",
    "cz": "git cz",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && yarn"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.1.0-rc.0",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "jsbarcode": "^3.11.5",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "vue": "^3.2.31",
    "vue-draggable-next": "^2.1.1",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2",
    "vxe-table": "^4.2.0",
    "xe-ajax": "^4.0.5",
    "xe-utils": "^3.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/compiler-sfc": "^3.2.31",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.6",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.2",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.6",
    "vite-plugin-html": "^3.2.0",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.33.2-patch.1",
    "vxe-table-plugin-antd": "^3.0.5"
  },
  "peerDependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^3.1.0-rc.0",
    "dayjs": "^1.11.0",
    "jsbarcode": "^3.11.5",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.31",
    "vue-draggable-next": "^2.1.1",
    "vxe-table": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:1992106/scm-ui.git"
  },
  "homepage": "https://github.com/1992106/scm-ui",
  "keywords": [
    "vue3",
    "vite",
    "scm-ui"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1992106/scm-ui/issues"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.vue": [
      "prettier --write",
      "stylelint --fix",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{css,scss,sass,less}": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
